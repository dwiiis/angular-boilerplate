{
  "projectType": "library",
  "sourceRoot": "libs/model/src",
  "prefix": "nglibs",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/model"],
      "options": {
        "jestConfig": "libs/model/jest.config.ts",
        "passWithNoTests": true
